{
  "name": "autoblogger-backend",
  "version": "2.0.0",
  "description": "AutoBlogger AI-Powered Blog Generator - Backend API",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
